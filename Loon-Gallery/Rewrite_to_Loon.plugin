[Script]

QX_Rewrite_Parser_10 = type=http-request, pattern=\/[^/]+\.[^/]+qx$|qx\?(jsc|hnadd|hndel|n|y|x|del|i)=, script-path=https://github.com/chengkongyiban/stash/raw/main/js/QX_Rewrite_Parser.js, requires-body=true, max-size=3145728, timeout=30

Surge_Rewrite_Parser_11 = type=http-request, pattern=\/[^/]+\.[^/]+sg$|sg\?(jsc|hnadd|hndel|n|y|x|del|i)=, script-path=https://github.com/chengkongyiban/stash/raw/main/js/Surge_Rewrite_Parser.js, requires-body=true, max-size=3145728, timeout=30

Loon_Rewrite_Parser_12 = type=http-request, pattern=\/[^/]+\.[^/]+loon$|loon\?(hndel|hnadd|n|y|x|del)=, script-path=https://github.com/chengkongyiban/stash/raw/main/js/Loon_Rewrite_Parser.js, requires-body=true, max-size=3145728, timeout=30

rule_parser_13 = type=http-request, pattern=r_parser.list$|r_parser.list\?.*, script-path=https://raw.githubusercontent.com/chengkongyiban/stash/main/js/rule_parser.js, requires-body=true, max-size=3145728, timeout=30

script-converter_14 = type=http-request, pattern=_script-converter-(stash|surge|loon|shadowrocket)\.js$, script-path=https://raw.githubusercontent.com/xream/scripts/main/surge/modules/script-converter/script-converter.js, requires-body=true, max-size=3145728, timeout=30

